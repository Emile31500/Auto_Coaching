<% title = 'Child Template' %>
<%- contentFor('mainNotMarged') %>
    
<%- contentFor('main') %>
<table class="table table-striped my-3">
    <thead class="table-dark">
        <tr>
            <th scope="col">
                Nom
            </th scope="col">
            <th scope="col">
                Sexe
            </th scope="col">
            <th scope="col">
                Message non vu
            </th scope="col">
        </tr>
    </thead>
    <tbody>
        <% for(var i=0; i < users.length; i++) { %>
            <tr>
                <td>
                    <a href="/admin/message/<%= users[i].id %>"><%= users[i].name %> </a>
                </td>
                <td>
                    <% if (users[i].sex) { %>
                        H
                    <% } else { %>
                        F
                    <% } %>
                </td>
                <td>
                    <span id="unseen-message-<%= users[i].id %>"> 
                        <span class="text-muted"> 0 </span> 
                    </span>
                </td>
            </tr> 
        <% } %>
    </tbody>
</table>
<br>
